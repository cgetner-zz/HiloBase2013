<div class="dashboard-top-content">
  <div class="" id="chart_header">
    <label><%=t ("emp_dashboard_graphs.heading") %>
      <span id="preview_chart_heading" class="chart-header"><%=t ("emp_dashboard_graphs.pos_view") %></span>
      <span id="interest_chart_heading" class="chart-header hidden"><%=t ("emp_dashboard_graphs.pos_applied") %></span>
      <span id="profile_chart_heading" class="chart-header hidden"><%=t ("emp_dashboard_graphs.profile_view") %></span>
      <span id="sharing_chart_heading" class="chart-header hidden"><%=t ("emp_dashboard_graphs.posting_sharing") %></span>
    </label>
  </div>

  <div class="summary-tabs-outer">
    <div class="summary-tabs-inner">
      <a href="javascript:void(0);" class="summary-tab summary-open-tab" data-section="preview_chart">a1</a>
      <a href="javascript:void(0);" class="summary-tab summary-close-tab" data-section="interest_chart">a2</a>
      <a href="javascript:void(0);" class="summary-tab summary-close-tab" data-section="profile_chart">a3</a>
      <a href="javascript:void(0);" class="summary-tab summary-close-tab" data-section="sharing_chart" style="margin-right:0">a4</a>
      <div class="clear-height"></div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function(){
    dashboardChartsSelect();
  });
</script>