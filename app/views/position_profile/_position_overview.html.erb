<script type="text/javascript">
  var compensation_arr  = eval("[<%= compensation_range.join(',')%>]");
  var paidtime_arr  = eval("[<%= paidtime_range.join(',')%>]");
  var commute_arr  = eval("[<%= commute_range.join(',')%>]");

  $(document).ready(function() {
    $('#position_parent_id').val($('#parent_id').val());
    $("input[type=text]").keyup(function(){
      if(!$(this).hasClass('edit-group-name ')) {
        $("#tab-1_hasThisFormBeenEdited").val(1);
      }
    });
    $("textarea").keyup(function(){
      $("#tab-1_hasThisFormBeenEdited").val(1);
    });
    $("#position-overview").bind("ajax:beforeSend",function(){showBlockShadow();});
    // Initialize radio buttons
    $("input['radio'].customize-radio").each(function() {
      $(this).hide();
      var onclick_behav = "toggleRadio(this);$('#tab-1_hasThisFormBeenEdited').val(1);";
      if($(this).is(':checked') == true){
        $(this).after('<a onclick="' + onclick_behav + '" class="custom-radio" style="float:left" href="javascript:void(0)"><img src="/assets/employer_v2/btn-radio-active.png"/></a>');
      }else{
        $(this).after('<a onclick="' + onclick_behav + '" class="custom-radio" style="float:left" href="javascript:void(0)"><img src="/assets/employer_v2/btn-radio-inactive.png"/></a>');
      }
    });

    // Removing Google API functionality of placeholders and restoring value
    $("#company_city").blur(function(){
      setTimeout(function(){
        if(!validateNotEmpty(document.getElementById('company_city'))){
          $('#company_city').removeAttr('placeholder');
          $('#company_city').val($('#company_city_placeholder').val());
        }
      }, 50);
    });

    $("#company_street_one").blur(function(){
      setTimeout(function(){
        if(!validateNotEmpty(document.getElementById('company_street_one'))){
          $('#company_street_one').removeAttr('placeholder');
          $('#company_street_one').val($('#company_street_one_placeholder').val());
        }
      }, 50);
    });

    // Checks if job is virtual, hides not required fields
    <%if job.remote_work == true %> $(".remote_work_fields").hide(); <% end %>
    BrowserDetect.init();
    if (BrowserDetect.browser == "Explorer" && BrowserDetect.version > "7") { $("#form-container").html(""); }
    if($("#attachmentpresent").val()=="true"){ increaseButtonHeight(); }
    $("#selected-position-name").text($("#selected-from-db").val());
    <%if selected_employment_ids.length > 0%> $("#selected-position-name").removeClass("placeholder"); <%end%>

    $('.state-slector').click(function(e) {
      if($(".state-options").is(':visible') == true) { hidePositionOverviewCustomSelectBox(); }
      else { $("#position-type-selector .state-slector").removeClass("error"); showPositionOverviewCustomSelectBox(); }
      e.stopPropagation();
    });

    $('body').click(function() { hidePositionOverviewCustomSelectBox(); });

    $('.option li').hover(function(){
      $('.option').children().each(function(){
        if($(this).attr('selected')=='selected'){ $(this).removeClass('selected').removeAttr('selected'); }
      });
      $(this).addClass('selected').attr('selected', 'selected');
    });

<%if job.profile_responsibilities.length == 0%> add_more_responsibility.add(); <%end%>
    add_more_responsibility.init();
    ownership_event.init();
    create_slider.call();    
<%  if job.minimum_compensation_amount != 0.0 and job.id != nil  %>
      mark_slider_values.call(<%=job.minimum_compensation_amount*1000%>,<%=job.maximum_compensation_amount*1000%>);
      calculate_compensation.initialize([<%=job.minimum_compensation_amount*1000%>,<%=job.maximum_compensation_amount*1000%>]);
<%  end %>
    adjustBasicJobTypeHeight();

    $("#hiring_company_true").next().click(function(){
      $("#hiring_company_name_container").hide();
      $("#hiring_company_name_container").removeClass("active-input").addClass("input-text");
      $("#hiring_company_name").val($("#hiring_company_name_placeholder").val());
    });

    $("#hiring_company_false").next().click(function(){
      $("#hiring_company_name_container").show();
      $("#hiring_company_name_container").removeClass("active-input").addClass("input-text");
      $("#hiring_company_name").val($("#hiring_company_name_placeholder").val());
    });

    $("#remote_work_true").next().click(function(){
      $(".remote_work_fields").hide();
