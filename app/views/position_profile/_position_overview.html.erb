<%= form_tag('/position_profile/create_new_emp_profile', :id => "position-overview", :remote => true) do %>
  <input type="hidden" id="company_group_id" name="company_group_id" value="<%= company_group_id %>" />
  <input type="hidden" name="company_state_flag" id="company_state_flag" value="0" />
  <input type="hidden" name="company_city_flag" id="company_city_flag" value="0" />
  <input type="hidden" name="company_street_one_flag" id="company_street_one_flag" value="0" />
  <input type="hidden" name="company_zip_flag" id="company_zip_flag" value="0" />
  <input type="hidden" name="company_country_flag" id="company_country_flag" value="0" />
  <input type="hidden" name="company_latitude" id="company_latitude" value="<% if not job_location.nil? %><%= job_location.latitude %><% end %>" />
  <input type="hidden" name="company_longitude" id="company_longitude" value="<% if not job_location.nil? %><%= job_location.longitude %><% end %>" />
  <input type="hidden" name="dropdown_check_flag" id="dropdown_check_flag" value="<% (!job_location.nil? && !job_location.city.to_s.empty?) ? '1' : '0' %>" />
  <input type="hidden" name="overview_complete" id="overview_complete" value="0" />
  <input type="hidden" name="job_id_save" id="job_id_save" value="<%= job.id if not job.nil? %>" />
  <input type="hidden" name="tab-1_hasThisFormBeenEdited" id="tab-1_hasThisFormBeenEdited" value="0" />
  <input type="hidden" name="position_parent_id" id="position_parent_id" value="0"/>
  <input type="hidden" name="street_address_check" id="street_address_check" value="0"/>
  <%=hidden_field_tag :compensation_value_min -%>
  <%=hidden_field_tag :compensation_value_max -%>
  <%=hidden_field_tag :desired_employment_ids -%>
  <%=hidden_field_tag :certificate_param%>
  <%=hidden_field_tag :selected_languages%>
  <%=hidden_field_tag :required_certificates -%>
  <%=hidden_field_tag :required_languages -%>
  <%=hidden_field_tag :required_degree %>
  <%=hidden_field_tag :selected_colleges %>
  <%=hidden_field_tag :degree_param %>
  <%=hidden_field_tag :selected_roles %>
  <input type="hidden" id="destination_path" name="destination_path" value="" />
  <!--accordion-content-->
  <!--Company Information-->
  <div class="multiple-block-cont">
    <div class="blocks position-info height-170px">
      <h4><%=t("position_overview.hiring_compnay.heading") %><img width="26" height="27" style="cursor:pointer;" class="rfloat" onclick="open_hiringcompany_popup();" src="/assets/position-preview-info.gif" /></h4>
      <span class="hiring-behalf"><%=t("position_overview.hiring_compnay.content") %></span>
      <ul class="comp-web-cls">
        <li>
          <div class="hiring-company-radio-btns margin-L-11px">
            <input type="radio" <%= (job.hiring_company.nil? || job.hiring_company == true) ? "checked = 'checked'" : "" %> value="1" name="hiring_company" class="customize-radio" id="hiring_company_true" style="display: none;" />
            <label style="float: left;margin-top: 4px; padding-left: 5px;margin-right: 3px; font-size:13px; font-weight:400; padding-right: 5px;"><%=t("position_overview.hiring_compnay.own") %></label>
          </div>
        </li>
        <li>
          <div class="hiring-company-radio-btns margin-L-11px">
            <input type="radio" <%= (!job.hiring_company.nil? && job.hiring_company == false) ? "checked = 'checked'" : "" %> value="0" name="hiring_company" class="customize-radio" id="hiring_company_false" style="display: none;" />
            <label style="float: left;margin-top: 4px; padding-left: 5px;margin-right: 3px; font-size:13px; font-weight:400;"><%=t("position_overview.hiring_compnay.thrid_party") %></label>
          </div>
        </li>
        <div id="hiring_company_name_container" class="customized-input <%if job.hiring_company_name.nil? %>input-text<% else %>active-input<% end %> margin-L-11px margin-T-10px height-170px" <% if !job.hiring_company.nil? && job.hiring_company == false %>style="display:block;"<% else %>style="display:none;"<% end %>>
          <div class="input-left"></div>
          <input type="hidden" id="hiring_company_name_placeholder" value="<%=t("position_overview.hiring_compnay.label") %>" />
          <input type="text" maxlength="50" class="hiring-comp-txt" name="hiring_company_name" id="hiring_company_name" onblur="blur_element(this);" onkeydown="type_element(this); " onfocus="focus_element(this);" value="<%= job.hiring_company_name.nil? ?  t("position_overview.hiring_compnay.label") : job.hiring_company_name %>" />
          <div class="input-right"></div>
        </div>
      </ul>
    </div>

    <div class="blocks responsibility">
      <h4>
        <%=t("position_overview.websites.heading") %>
        <span class="character-remaining"><%=t("position_overview.websites.optional") %></span>
      </h4>
      <div class="pp_error_message" id="web_err_msg"></div>
      <div class="company-information" style="margin-top:12px;">
        <ul class="comp-web-cls">

          <li class="webname_li">
            <div class="customized-input <% if job.website_one.blank? %>input-text<%else%>active-input<% end %> website-name">
              <div class="input-left">&nbsp;</div>
              <input id="website_1_placeholder" type="hidden" value="<%=t("position_overview.websites.label1") %>" />
              <input class="webname" maxlength="250" type="text" value="<%= job.website_one.blank? ? t("position_overview.websites.label1")  : job.website_one %>" id="website_1" name="website_1" onfocus="focus_element(this);" onblur="validatePositionPreviewOnBlur(this);blur_element(this);" onkeydown="type_element(this);" />
              <div class="input-right">&nbsp;</div>
            </div>
          </li>
          <li class="webtitle_li">
            <div class="customized-input <% if job.website_title_one.blank? %>input-text<%else%>active-input<% end %> website-title">
              <div class="input-left">&nbsp;</div>
              <input type="hidden" value="<%=t("position_overview.websites.label2") %>" id="website_title_1_placeholder"  />
              <input class="webtitle" maxlength="20" type="text" value="<%= job.website_title_one.blank? ? t("position_overview.websites.label2")  : job.website_title_one %>" id="website_title_1" name="website_title_1" onfocus="focus_element(this);" onblur="blur_element(this);" onkeydown="type_element(this);" />
              <div class="input-right">&nbsp;</div>
            </div>
          </li>

          <li class="webname_li">
            <div class="customized-input <% if job.website_two.blank? %>input-text<%else%>active-input<% end %> website-name">
              <div class="input-left">&nbsp;</div>
              <input id="website_2_placeholder" type="hidden" value="<%=t("position_overview.websites.label1") %>"  />
              <input class="webname" maxlength="250" type="text" value="<%= job.website_two.blank? ? t("position_overview.websites.label1") : job.website_two %>" id="website_2" name="website_2" onfocus="focus_element(this);" onblur="validatePositionPreviewOnBlur(this);blur_element(this);" onkeydown="type_element(this);" />
              <div class="input-right">&nbsp;</div>
            </div>
          </li>
          <li class="webtitle_li">
            <div class="customized-input <% if job.website_title_two.blank? %>input-text<%else%>active-input<% end %> website-title">
              <div class="input-left">&nbsp;</div>
              <input type="hidden" value="<%=t("position_overview.websites.label2") %>" id="website_title_2_placeholder"  />
              <input class="webtitle" maxlength="20" type="text" value="<%= job.website_title_two.blank? ? t("position_overview.websites.label2")  : job.website_title_two %>" id="website_title_2" name="website_title_2" onfocus="focus_element(this);" onblur="blur_element(this);" onkeydown="type_element(this);" />
              <div class="input-right">&nbsp;</div>
            </div>
          </li>

          <li class="webname_li">
            <div class="customized-input <% if job.website_three.blank? %>input-text<%else%>active-input<% end %> website-name">
              <div class="input-left">&nbsp;</div>
              <input id="website_3_placeholder" type="hidden" value="<%=t("position_overview.websites.label1") %>" name="website_placeholder"  />
              <input class="webname" maxlength="250" type="text" value="<%= job.website_three.blank? ? t("position_overview.websites.label1")  : job.website_three %>" id="website_3" name="website_3" onfocus="focus_element(this);" onblur="validatePositionPreviewOnBlur(this);blur_element(this);" onkeydown="type_element(this);" />
              <div class="input-right">&nbsp;</div>
            </div>
          </li>
          <li class="webtitle_li">
            <div class="customized-input <% if job.website_title_three.blank? %>input-text<%else%>active-input<% end %> website-title">
              <div class="input-left">&nbsp;</div>
              <input type="hidden" value="<%=t("position_overview.websites.label2") %>" id="website_title_3_placeholder"  />
              <input class="webtitle" maxlength="20" type="text" value="<%= job.website_title_three.blank? ? t("position_overview.websites.label2")  : job.website_title_three %>" id="website_title_3" name="website_title_3" onfocus="focus_element(this);" onblur="blur_element(this);" onkeydown="type_element(this);" />
              <div class="input-right">&nbsp;</div>
            </div>
          </li>
        </ul>
      </div>
    </div>

    <div id="hiring-company-popup" class="hiring-company-tooltip" style="display:none;">
      <div class="hiring-company-tooltip-right"><img src="/assets/employer_v2/hiring-popup-arrow.png" style="height:21px;width:12px;margin-top: 31px;" /></div>
      <div class="hiring-company-tooltip-content">
        <label><%=t("position_overview.hiring_compnay.pop_up.heading") %></label>
        <div class="separator"></div>
        <p><%=t("position_overview.hiring_compnay.pop_up.contentL1") %></p>
        <p><%=t("position_overview.hiring_compnay.pop_up.contentL2") %><b><%=t("position_overview.hiring_compnay.pop_up.contentL2B") %></b><%=t("position_overview.hiring_compnay.pop_up.contentL3") %></p>
        <div class="separator"></div>
      </div>
    </div>
  </div>
  <div id="fade_normal_status_hiring-company" class="black_overlay_light" onclick="close_hiringcompany_popup();"></div>
  <!--Company Information End-->

  <!--multiple-block-cont-->
  <div class="multiple-block-cont">
    <!--blocks-->
    <div class="blocks position-info height-367px">
      <h4><%=t("position_overview.position_info.heading") %><img class="rfloat" width="26" height="27" src="/assets/position-preview-info.gif" onclick="open_position_information_popup();" style="cursor:pointer;" /></h4>
      <div class="pp_two_error_message" id="pi_error_msg" style="width: 165px;height: 14px;">&nbsp;</div>
      <div class="company-information margin-T-3px overflow-visible">
        <ul>
          <li>
            <div class="customized-input position-title <%if(job.nil? || job.name.to_s.empty?) %>input-text<% else %>active-input<% end %>">
              <div class="input-left"></div>
              <input type="hidden" id="name_placeholder" value="<%=t("position_overview.position_info.pos_title") %>" />
              <input class="<% if(job.nil? || job.name.to_s.empty?) %><% else %>filled-input<% end %>" id="name" maxlength="60" name="name" type="text" value="<%= job.name.nil? ? t("position_overview.position_info.pos_title")  : job.name %>" onfocus="focus_element(this)" onblur="blur_element(this)" onkeydown="type_element(this)"/>
              <% if !job.id.blank? %>
                <%= hidden_field_tag :id, job.id%>
              <% end %>
              <div class="input-right"></div>
            </div>
          </li>
        </ul>
        <ul>
          <li>
            <!-- CUSTOM SELECT -->
            <div class="position-info" id="position-type-selector">
              <div class="position-type-selector">
                <div class="state-slector">
                  <span id="selected-position-name" class="placeholder label-default" style="margin-top:3px;color:#000066;display: block; float: left; width: 170px;"><%=t("position_overview.position_info.type") %></span>
                  <div class="position-aero" style="float: right; width: 20px; height: 22px;"></div>
                  <input type="hidden" id="desired_employments" name="desired_employments[]" value="<%= selected_employment_ids[0].to_i %>" />
                  <% for desired_employment in desired_employments %>
                    <% if selected_employment_ids[0].to_i == desired_employment.id %>
                      <input type="hidden" id="selected-from-db" value="<%=desired_employment.name%>" />
                    <% end %>
                  <% end %>
                  <div style="display:none;" id="position_overview_options" class="state-options">
                    <div class="state-options-top">&nbsp;</div>
                    <div class="state-options-content">
                      <label class="label-default" style="width:184px;"><%=t("position_overview.position_info.type") %></label>
                      <ul class="option" id="position-list">
                        <% for desired_employment in desired_employments %>
                          <li selected="" class="Position-TypeDD" value="<%=desired_employment.id%>" onclick='$("#selected-position-name").removeClass("placeholder");$("#selected-position-name").text("<%=h(desired_employment.name)%>");$("#desired_employments").val("<%=desired_employment.id%>");$("#tab-1_hasThisFormBeenEdited").val(1);'><label id="<%= desired_employment.id %>"><%=h(desired_employment.name)%></label></li>
                        <% end %>
                      </ul>
                    </div>
                    <div class="state-options-bottom">&nbsp;</div>
                  </div>
                </div>
              </div>
            </div>
            <!-- CUSTOM SELECT -->
          </li>
        </ul>
        <ul>
          <li style="margin-bottom: 7px;margin-top:9px">
            <div class="personal-radio-btns" style="margin-top:2px;">
              <input id="remote_work_false" class="customize-radio" type="radio" name="remote_work" value="0" <%= (job.remote_work.nil? || job.remote_work == false) ? "checked = 'checked'" : "" %>/>
              <label style="float: left;margin-top: 4px; padding-left: 5px;margin-right: 3px; font-size:13px; font-weight:400; padding-right: 5px;"><%=t("position_overview.position_info.in_office") %></label>
              <input id="remote_work_true" class="customize-radio" type="radio" name="remote_work" value="1" <%= job.remote_work == true ? "checked = 'checked'" : "" %>/>
              <label style="float: left;margin-top: 4px; padding-left: 5px;margin-right: 3px; font-size:13px; font-weight:400;"><%=t("position_overview.position_info.virtual") %></label>
            </div>
          </li>

          <li class="remote_work_fields">
            <div class="customized-input street-area <% if(job_location.nil? || job_location.street_one.to_s.empty?) %>input-text<%else%>active-input<%end%>">
              <div class="input-left"></div>
              <input type="hidden" id="company_street_one_placeholder" value="<%=t("position_overview.position_info.street_add") %>" />
              <input id="company_street_one" class="<% if(job_location.nil? || job_location.street_one.to_s.empty?)%><%else%>filled-input<%end%>" name="company_street_one" value="<%= job_location.nil? || job_location.street_one.to_s.empty? ? t("position_overview.position_info.street_add")  :  job_location.street_one%>" type="text" onfocus="focus_element(this)" onblur="blur_element(this)" onkeydown="type_element(this)" onkeyup="$('#street_address_check').val('1');"/>
              <div class="input-right"></div>
            </div>
          </li>

          <li class="remote_work_fields">
            <div class="customized-input company_city <% if(job_location.nil? || job_location.city.to_s.empty?)%>input-text<%else%>active-input<%end%>">
              <div class="input-left"></div>
              <input type="hidden" id="company_city_placeholder" value="<%=t("position_overview.position_info.city") %>" />
              <input type="text" name="company_city" id="company_city" value="<%= job_location.nil? || job_location.city.to_s.empty? ? t("position_overview.position_info.city")  : job_location.city.force_encoding('utf-8') %>" autocomplete="off" onfocus="focus_element(this)" onblur="validatePositionPreviewOnBlur(this);blur_element(this);" onkeydown="type_element(this);$('#dropdown_check_flag').val(0);"/>
              <div class="input-right"></div>
            </div>
          </li>

          <li class="remote_work_fields">
            <div class="customized-input company_state <% if(job_location.nil? || job_location.state.to_s.empty?)%>input-text<%else%>active-input<%end%>">
              <div class="input-left"></div>
              <input type="hidden" id="company_state_placeholder" value="<%=t("position_overview.position_info.state") %>" />
              <input type="text" class="<% if(job_location.nil? || job_location.state.to_s.empty?)%><%else%>filled-input<%end%>" value="<%= job_location.nil? || job_location.state.to_s.empty? ? t("position_overview.position_info.state")  : job_location.state.force_encoding('utf-8') %>" id="company_state" name="company_state" onfocus="focus_element(this)" onblur="blur_element(this)" onkeydown="type_element(this)"/>
              <div class="input-right"></div>
            </div>
          </li>

          <!-- Zip Code -->
          <li class="remote_work_fields">
            <div class="customized-input zip-code <% if(job_location.nil? || job_location.zip_code.to_s.empty?)%>input-text<%else%>active-input<%end%>">
              <div class="input-left"></div>
              <input type="hidden" id="company_zip_placeholder" value="<%=t("position_overview.position_info.zip") %>" />
              <input type="text" class="<% if(@job_location.nil? || @job_location.zip_code.to_s.empty?)%><%else%>filled-input<%end%>" value="<%=@job_location.nil? || @job_location.zip_code.to_s.empty? ? t("position_overview.position_info.zip")  : @job_location.zip_code%>" id="company_zip" name="company_zip" onfocus="focus_element(this)" onblur="blur_element(this)" onkeydown="type_element(this)" maxlength="10"/>
              <div class="input-right"></div>
            </div>
          </li>
          <!-- Country -->
          <li class="remote_work_fields">
            <div class="customized-input company_country <% if(job_location.nil? || job_location.country.to_s.empty?)%>input-text<%else%>active-input<%end%>">
              <div class="input-left"></div>
              <input type="hidden" id="company_country_placeholder" value="<%=t("position_overview.position_info.country") %>" />
              <input type="text" class="<% if(job_location.nil? || job_location.country.to_s.empty?)%><%else%>filled-input<%end%>" value="<%= job_location.nil? || job_location.country.to_s.empty? ? t("position_overview.position_info.country")  : job_location.country.force_encoding('utf-8') %>" id="company_country" name="company_country" onfocus="focus_element(this)" onblur="blur_element(this)" onkeydown="type_element(this)"/>
              <div class="input-right"></div>
            </div>
          </li>
        </ul>
      </div>
    </div>
    <!--/blocks-->

    <!--blocks-->
    <div class="blocks responsibility">
      <h4>
        <%=t("position_overview.responsibilities.heading") %>
        <span class="character-remaining">(Unranked)</span>
        <span class="character-remaining rfloat" id="resp_char_remain"><%=t("position_overview.responsibilities.char_remaining") %></span>
      </h4>
      <div class="pp_error_message">&nbsp;</div>
      <div class="company-information">
        <ul class="daily-repo">
          <%j = 1
          for pr in job.profile_responsibilities.all(:order => "job_profile_responsibilities.id")%>
            <li class="tto">
              <div class="customized-input <%if pr.name.to_s.empty?%>input-text<%else%>active-input<%end%>">
                <div class="input-left"></div>
                <input type="hidden" id="resp_<%=j%>_placeholder" value="<%=t("position_overview.responsibilities.label1") %><%=j %> <% if j!=1 %><%=t("position_overview.responsibilities.label2") %><%end%> " />
                <input type="text" id="resp_<%=j%>" maxlength="60" class="<%if pr.name.to_s.empty?%><%else%>filled-input<%end%>" value="<%=pr.name%>" name="responsiblities[]" onfocus="focus_element(this);calculateChars(this, 60);" onkeydown="type_element(this); " onblur="blur_element(this);" onkeyup="calculateChars(this, 60);" />
                <div class="input-right"></div>
              </div>
            </li>
            <%break if j == 5
            j = j + 1
          end%>
          <%(job.profile_responsibilities.length + 1).upto(5) do |i|%>
            <li>
              <div class="customized-input input-text">
                <div class="input-left"></div>
                <input type="hidden" id="resp_<%=i%>_placeholder" value="<%=t("position_overview.responsibilities.label1") %><%=i%> <% if i!=1 %><%=t("position_overview.responsibilities.label2") %><%end%>" class="plc"/>
                <input type="text" id="resp_<%=i%>" maxlength="60" value="<%=t("position_overview.responsibilities.label1") %><%=i%> <% if i!=1 %><%=t("position_overview.responsibilities.label2") %><%end%>" name="responsiblities[]"  onfocus="focus_element(this); calculateChars(this, 60);" onkeydown="type_element(this);" onblur="blur_element(this);" onkeyup="calculateChars(this, 60);"/>
                <div class="input-right"></div>
              </div>
            </li>
          <%end%>
        </ul>
      </div>
      <!--Position information pop up blocks-->
      <div id="position-information-popup" class="position-information-tooltip" style="display:none;">
        <div class="position-information-tooltip-right"><img src="/assets/employer_v2/hiring-popup-arrow.png" style="height:21px;width:12px;margin-top: 8px;" /></div>
        <div class="position-information-tooltip-content">
          <label><%=t("position_overview.position_info.pop_up.heading") %></label>
          <div class="separator"></div>
          <p><%=t("position_overview.position_info.pop_up.contentL1") %></p>
          <p><%=t("position_overview.position_info.pop_up.contentL2") %><b><%=t("position_overview.position_info.pop_up.contentL2B") %></b><%=t("position_overview.position_info.pop_up.contentL3") %></p>
          <p><%=t("position_overview.position_info.pop_up.contentL4") %><b><%=t("position_overview.position_info.pop_up.contentL4B") %></b><%=t("position_overview.position_info.pop_up.contentL5") %></p>
          <div class="separator"></div>
        </div>
      </div>
      <!--Position information pop up blocks Ends-->
    </div>
    <div id="fade_normal_status_position-information" class="black_overlay_light" onclick="close_position_information_popup();"></div>
    <!--/blocks-->
    <!--U. S. ARMED FORCES blocks-->
    <div class="blocks responsibility">
      <h4><%=t("position_overview.us_armed_force.heading")%></h4>
      <div class="company-information">
        <div class="US-armed-text"><%=t("position_overview.us_armed_force.content")%></div>
        <div class="us-radio-btns">
          <input type="radio" class="customize-radio" id="us_armed_forces_yes" name="armed_forces" value="1" <%= (job.armed_forces.nil? || job.armed_forces == true) ? "checked = 'checked'" : "" %>/>
          <label style="float: left;margin-top: 4px; padding-left: 5px;width:65px;"><%=t("position_overview.us_armed_force.yes")%></label>
