<!-- Beta Code Access -->
<div id="beta_access" class="white_content">
  <div class="popup-upper-block-fogetPwd">
    <span class="popup-top">&nbsp;</span>
    <div class="popup-content">
      <div class="gradient-content">
        <div class="popup-header-container">
          <label class="popup-heading"><%= t('beta_access.heading') %></label>
        </div>
        <div class="access-block">
          <div class="lfloat normal-text" style="text-align: justify; width: 365px;">
            <label class="free-text">
              <%= t('beta_access.free_text') %>
            </label>
          </div>
          <div class="rfloat" style="margin-top: 112px;">
            <input type="button" class="continue-button-active" onclick="$('#beta_access').hide();$('#fade_normal').hide();" />
          </div>
        </div>
      </div>
      <a href="javascript:void(0)" class="close" onClick="$('#beta_access').hide();$('#fade_normal').hide();">Close</a>
    </div>
    <span class="popup-bottom">&nbsp;</span>
  </div>
</div>
<!-- /Beta Code Access -->