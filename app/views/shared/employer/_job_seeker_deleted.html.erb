<div class="one_click_payment_position_white_content" id="job_seeker_deleted_popup" style="display:none; position: fixed;">
  <div class="dashboard_position_popup-siteActivation">
    <span class="position_popup-top">&nbsp;</span>
    <div class="position_popup-content_oneclick">
      <div class="position_gradient-content_oneclick">
        <div class="position_popup-header-container">
          <label class="popup-heading"><%=t ("career_seeker_deleted_popup.title") %></label>
        </div>

        <div class="position_popup_select_box">
          <div class="access-block">
            <div class="lfloat error-text" style="width: 32%;">
              <label class="free-text"></label>
            </div>
            <div class="rfloat" id="action-button" style="margin-left:0px;">
              <input type="button" id="job_seeker_deleted_popup_button" class="ok-button-active" onclick="closeJobSeekerDeletedPopup();"/>
            </div>
          </div>
        </div>
      </div>
      <a id="save-close" href="javascript:void(0)" class="position_close_oneclick" onclick="closeJobSeekerDeletedPopup();"><%=t ("career_seeker_deleted_popup.close") %></a>
    </div>
    <span class="position_popup-bottom">&nbsp;</span>
  </div>
</div>