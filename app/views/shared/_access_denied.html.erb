    <span class="position_popup-top">&nbsp;</span>
    <div class="position_popup-content_oneclick">
      <div class="position_gradient-content_oneclick">
        <div class="position_popup-header-container">
          <label class="popup-heading"><%=t ("position_authorize_popup.title") %></label>
        </div>

        <div class="position_popup_select_box">
          <div class="access-block">
            <div class="lfloat error-text" style="width: 32%;">
              <label class="free-text"><%=t ("position_authorize_popup.content") %></label>
            </div>
            <div class="rfloat" id="action-button" style="margin-left:0px;">
              <input type="button" id="ok-button-active_button" class="ok-button-active" onclick="closeAcccessDenied();"/>
            </div>
          </div>
        </div>
      </div>
      <a id="save-close" href="javascript:void(0)" id="permission_denied_button" class="position_close_oneclick" onclick="closeAcccessDenied();"><%=t ("position_authorize_popup.close") %></a>
    </div>
    <span class="position_popup-bottom">&nbsp;</span>
  </div>
