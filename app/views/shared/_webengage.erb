<!-- Script for webengage feedback system-->
<script id="_webengage_script_tag" type="text/javascript">
  window.webengageWidgetInit = window.webengageWidgetInit || function(){
    webengage.init({
      licenseCode:"<%=$WEBENGAGE_LICENSE_CODE%>"
    }).onReady(function(){
      webengage.feedback.render({
        data: {
          name : "<% if not session[:job_seeker].nil? %><%=session[:job_seeker].first_name%> <%=session[:job_seeker].last_name%><%elsif not session[:employer].nil? %><%=session[:employer].first_name%> <%=session[:employer].last_name%><% else %><% end %>",
          email : "<% if not session[:job_seeker].nil? %><%=session[:job_seeker].email%><%elsif not session[:employer].nil? %><%=session[:employer].email%><% else %><% end %>"
        }
      });
      webengage.survey.render();
      webengage.notification.render();
    });
  };
  (function(d){
    var _we = d.createElement('script');
    _we.type = 'text/javascript';
    _we.async = true;
    _we.src = (d.location.protocol == 'https:' ? "//ssl.widgets.webengage.com" : "//cdn.widgets.webengage.com") + "/js/widget/webengage-min-v-3.0.js";
    var _sNode = d.getElementById('_webengage_script_tag');
    _sNode.parentNode.insertBefore(_we, _sNode);
  })(document);
</script>
<!-- /Script for webengage feedback system-->