                  <div id="fade_error_warning" class="error_overlay" style="z-index: 1003" onclick="clickOutside(this);"></div>
                  <div id="fade_normal_warning" class="black_overlay" style="z-index: 1003" onclick="clickOutside(this);"></div>
                  <div id="fade_success_sharing" class="sucess_overlay" style="z-index: 1003" onclick="clickOutside(this);"></div>
                  <input type="hidden" id="page_url_job" value="<%=CGI::escape(request.url)%>"></input>

                  <%= render :partial => "/home/gift_hilo_registered_signin_failure" %>
                  <%= render :partial => "/home/gift_hilo_one_click_signin_failure" %>
                  <%= render :partial => "/home/gift_hilo_signin_failure" %>
                  <%= render :partial => "/home/gift_hilo_failure" %>
                  <div id="gift_hilo_popup" class="white_content"></div>
                  <div id="gift_hilo_success_msg" class="white_content"></div>
                  <div id="checkout_billing_info" class="white_content"></div>
                  <div id="gift_cc_billing_popup" class="white_content"></div>

                  <!--Seeker Analyzer pop up-->
                  <div id="envNrole-cont-seeker-analyzer-popup" class="seeker-analyzer-popup" style="display:none;">
                    <div class="seeker-analyzer-leftdiv">
                      <div class="seeker-analyzer-popup-top">&nbsp;</div>
                      <div class="seeker-analyzer-popup-content">
                        <div id="popup-content-slider"></div>
                      </div>
                      <div class="seeker-analyzer-bottom">&nbsp;</div>
                    </div>

                    <div class="seeker-analyzer-rightdiv">
                      <img src="/assets/seeker-communicator-arrow.png" height="25px" width="17px"/>
                    </div>
                  </div>
                  <!--End here Seeker Analyzer pop up-->
                  <%= render :partial => "/shared/webengage" %>
                  </body>
                  </html>
                  <script type="text/javascript">
<%if session[:job_seeker]%>
    var login_section_type = "job_seeker";
<%end%>
   
  document.onload = loading();
   
  function loading(){
<% if session[:account_auth] == true and controller.action_name != "account_info" %>
      showAuthenticationPopup();
  <% session[:account_auth] = nil %>
<% end %>

<% if session[:upload_failure] == true %>
      $("#upload-error").show();
      showErrorShadow();
  <% session[:upload_failure] = nil %>
<% end %>
    centralizePopup();

<% if session[:success_job_seeker_msg] %>
      hideBlockShadow();
      show_job.call(<%=session[:share_job_id]%>);
  <% if not session[:job_seeker_msg].nil? %>
        showSuccessSharingJSMsg('<%= session[:job_seeker_msg] %>');
  <% end %>
  <%  session[:url] = nil %>
  <%  session[:share_job_id] = nil %>
  <% session[:job_seeker_msg] = nil %>
  <% session[:success_job_seeker_msg] = nil %>
<% end %>

<% if session[:success_share] %>
      hideBlockShadow();
  <% if  not session[:hilo_msg].nil? %>
        showSuccessHiloSharingMsg('<%= session[:hilo_msg] %>')
  <% end %>
  <%  session[:url_hilo] = nil %>
  <% session[:hilo_msg] = nil %>
  <% session[:success_share] = nil %>
<% end %>

<% if session[:success_share_gift] %>
      hideBlockShadow();
      click_gift_hilo.gift_hilo_show();
  <% if  not session[:hilo_msg].nil? %>
        showSuccessSharingJSMsg('<%= session[:hilo_msg] %>');
  <% end %>
  <% session[:gift_share] = nil%>
  <%  session[:url_hilo] = nil %>
  <% session[:hilo_msg] = nil %>
  <% session[:success_share_gift] = nil %>
<% end %>

<% if session[:success_share_about] %>
      hideBlockShadow();
      showAboutHilo();
  <% if  not session[:hilo_msg].nil? %>
        showSuccessSharingJSMsg('<%= session[:hilo_msg] %>');
  <% end %>
  <% session[:gift_share] = nil%>
  <%  session[:url_hilo] = nil %>
  <% session[:hilo_msg] = nil %>
  <% session[:success_share_about] = nil %>
<% end %>

<% if session[:gift_express] %>
      hideBlockShadow();
      hideNormalShadow();
      $('#gift_verify-loader-img').hide();
      $('#gifts_payment_verify_button').show();
      $("#checkout_billing_popup").hide();
      $("#gift_cc_billing_popup").empty();
      showSuccessShadow();
      showSuccessGiftMsg();
      centralizePopup();
  <% session[:gift_express] = nil %>
  <% session[:gift_url] = nil%>
<% end %>

  }
                  </script>