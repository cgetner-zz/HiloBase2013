                  <div id="fade_white" class="white_overlay"></div>
                  <% if controller.controller_name == "questionnaire" %>
                    <div id="fade_block_quest" class="block_overlay" style="display:block;"><center><div class="ajax_loading"><div class="ajax_loading_msg">Loading... Please Wait!</div></div></center></div>
                  <% end %>
