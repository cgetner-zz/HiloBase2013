                  <%= render :partial => "/gift/complete_payment_no_one_click" %>

                  <% if controller.controller_name == "pairing_profile" %>
                    <%= render :partial => "pairing_profile/advanced_popup" -%>
                    <%= render :partial => "pairing_profile/credential_help_popup" %>
                    <%= render :partial => "pairing_profile/credential_help_popup2" %>
                    <%= render :partial => "pairing_profile/credential_help_popup3" %>
                    <%= render :partial => "pairing_profile/credential_help_popup4" %>
                    <% if controller.action_name == "credentials" %>
                      <%= render :partial => "pairing_profile/military", :locals => {:job_seeker => @job_seeker} %>
                    <% end %>
                    <% if controller.action_name == "basics" %>
                      <%= render :partial => "pairing_profile/pairing_profile_complete" %>
                      <%= render :partial => "pairing_profile/cant_find_promo" %>
                      <%= render :partial => "home/beta_access_error" %>
                      <%= render :partial => "home/beta_access_success" %>
                    <% end %>
                    <% if controller.action_name == "credentials" %>
                      <%= render :partial => "pairing_profile/explore_roles" %>
                    <% end %>
                  <% end %>
                  <% if controller.controller_name == "payment" %>
                    <%= render :partial => "pairing_profile/cant_find_promo" %>
                    <%= render :partial => "home/beta_access_error" %>
                    <%= render :partial => "home/beta_access_success" %>
                  <% end %>
                  <% if controller.controller_name == "questionnaire" %>
                    <%= render :partial => "questionnaire/questionnaire_complete" %>
                  <% end %>

                  <% if controller.controller_name == "account" %>
                    <%= render :partial => "account/save_search" %>
                    <%= render :partial => "account/following_confirmation" %>
                    <%= render :partial => "pairing_profile/advanced_popup" -%>
                    <%= render :partial => "pairing_profile/credential_help_popup" %>
                    <%= render :partial => "pairing_profile/credential_help_popup2" %>
                    <%= render :partial => "pairing_profile/credential_help_popup3" %>
                    <%= render :partial => "pairing_profile/credential_help_popup4" %>
                    <%= render :partial => "account/status_popup" -%>
                    <%= render :partial => "account/authentication_popup" %>
                    <%= render :partial => "account/auth_error" %>
                    <%= render :partial => "/home/job_not_active" %>
                    <%= render :partial => "/job_payment/complete_purchase_no_one_click" %>
                    <%= render :partial => "/job_payment/complete_purchase_one_click" %>
                    <%= render :partial => "account/legacy_data_popup" %>
                    <%= render :partial => "/job_payment/not_authorized" %>

                    <% if controller.action_name == "pairing_profile" %>
                      <%= render :partial => "pairing_profile/explore_roles" %>
                      <%= render :partial => "account/intro_popup" %>
                      <%= render :partial => "account/areYouSure" %>
                      <div id="skills_ajax"><%= render :partial => "account/skills_popup" %></div>
                      <div id="employer_view_popup" class="white_content"></div>
                      <%= render :partial => "account/upload_failure" %>
                    <% end %>
                    <% if controller.action_name == "account_info" %>
                      <%= render :partial => "account/code_conformation_popup" %>
                      <%= render :partial => "account/code_error_popup" %>
                      <%= render :partial => "account/delete_confirmation_popup" %>
                      <div id="ajax-content"></div>
                    <% end %>

                    <div id="notification"></div>
                    <div id="position_details" class="detiled_content"></div>
                    <div id="position_overview" class="white_content"></div>
                    <div id="cc_billing_popup" class="white_content"></div>
                    <div id="job_preview_details_popup"></div>
                    <div id="old_pass_mismatch" class="white_content"></div>
                    <div id="update_emailexist_error_popup" class="white_content"></div>
                    <div id="file_upload_ie" class="white_content"></div>
                    <%= render :partial => "account/birkman_pdf_download_error" -%>
                    <%= render :partial => "account/career_code" -%>
                    <%= render :partial => "account/career_code_error" -%>
                    <%= render :partial => "account/account_popup" %>
                    <%= render :partial => "account/one_click_login_error" %>
                    <%= render :partial => "account/ajax_error" %>
                    <%= render :partial => "account/pdf_ajax_error" %>
                    <%= render :partial => "account/ajax_payment_error_popup" %>
                    <%= render :partial => "account/congratulate_popup" %>
                    <%= render :partial => "account/job_expiry_popup" %>
                    <%= render :partial => "account/job_inactive_popup" %>
                    <% #render :partial => "job_payment/credit_card_popup", :locals => {:promo_consumed_amount => nil, :remianing_amount => nil} %>
                  <% end %>
                  <% if controller.controller_name == "job_seeker" or controller.controller_name == "hilo_search"%>
                    <%= render :partial => "home/login_failure_popup" %>
                    <%= render :partial => "/home/forgot_password" %>
                  <% end %>
                  <% if controller.controller_name == "hilo_search" %>
                    <%= render :partial => "/hilo_search/hilo_search_work_env_job_msg" %>
                    <div id="hilo_search_job_seekers_few_records" class="white_content"></div>
                  <% end %>
                  <div id="fade_normal_status_skill-tooltip" class="black_overlay_skill" onclick="close_skill_tooltip_popup();close_cert_tooltip_popup();"></div>
                  <div id="fade_normal_status_seeker_analyzer" class="black_seeker_analyzer" onclick="close_seeker_analyzer_popup();"></div>
                  <div id="fade_normal_status" class="black_overlay_light" onclick="clickOutside(this);"><span class="popup-loader-light" style="display:none; float:left; margin-left: 50%; margin-top: 20%;" class="loader"><%=image_tag("ajax-loader-login-50p.gif")%></span></div>
                  <div id="fade_normal" class="black_overlay" onclick="clickOutside(this);"><span class="popup-loader" style="display:none; float:left; margin-left: 50%; margin-top: 20%;"><%=image_tag("ajax-loader-login-grey.gif")%></span></div>
                  <div id="fade_normal_dark" class="black_overlay_dark" onclick="clickOutside(this);"><span class="popup-loader" style="display:none; float:left; margin-left: 50%; margin-top: 20%;"><%=image_tag("ajax-loader-login-grey.gif")%></span></div>
                  <div id="fade_success" class="sucess_overlay" onclick="clickOutside(this);"></div>
                  <div id="fade_block" class="block_overlay"><center><div class="ajax_loading"><div class="ajax_loading_msg">Loading... Please Wait!</div></div></center></div>
