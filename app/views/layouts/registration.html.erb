  <body id="inner-Page" oncontextmenu="">
    <% if(controller.controller_name == "job_seeker") %>
      <!--page-container-->
      <div id="page-container-accountSetup">
      <% elsif controller.controller_name == "questionnaire" or controller.controller_name == "hilo_search" %>
        <!--page-container-->
        <div id="page-container-personalityQuest">
