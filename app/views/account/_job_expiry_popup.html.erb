<!-- Job Expiry Popup -->
<div id="job_expiry_error" class="white_content" style="z-index: 1008; top: 15%; position: fixed;">
  <div class="popup-upper-block-fogetPwd">
    <span class="popup-top">&nbsp;</span>
    <div class="popup-content">
      <div class="gradient-content">
        <div class="popup-header-container">
          <label class="popup-heading job_expiry_warning"></label>
        </div>
        <div class="access-block">
          <div class="lfloat error-text">
            <label class="free-text job_expiry_msg">
              <span style="font-weight: 400; font-size: 14px;"><%= t('job_expiry_error.free_text1') %><br /><%= t('job_expiry_error.free_text2') %></span>
              <input type="hidden" name="pay_for" id="pay_for" />
              <input type="hidden" name="job_id_value" id="job_id_value" />
            </label>
          </div>
          <div class="rfloat margin-T-5px">
            <div class="lfloat"><input type="button" class="cancel-button-active" onclick="hideErrorCancel();" /></div>
            <div class="lfloat"><input id ="job_expiry_error_continue" type="button" class="continue-button-active" onclick="hideErrorPopup(<%= EXCLUDE_PAYMENT %>);" style="margin-left: 10px; margin-top: 1px;" /></div>
          </div>
        </div>
      </div>

      <div class="home-popup-contnet">

      </div>
      <a href="javascript:void(0)" class="close" onClick="hideErrorCancel();">Close</a>
    </div>
    <span class="popup-bottom">&nbsp;</span>
  </div>
</div>
<!-- Job Expiry popup code ends here -->
