<style type="text/css">
  ul#toolbar-personality { height: 13px;width: auto;align: center;text-align: center;list-style: none;margin: 5px 44px 0 0;clear: both;position: relative; float: right;}
  #toolbar-personality li { position: relative; text-indent: 0px; top: 0; float: left;}
  #toolbar-personality li a { height: 13px;display: block;outline:none; width: 13px;}
  #toolbar-personality li.active { background-position: bottom;height: 10px;z-index: 10;outline:none; }
  #toolbar-personality li.active a{background: url("/assets/active-blue.png") no-repeat scroll 0 0 transparent; height: 13px; width: 13px;}
  #toolbar-personality li.inactive { background-position: top;border-bottom: none;height: 10px; }
  #toolbar-personality li.inactive a{background: url("/assets/inactive-blue.png") no-repeat scroll 0 0 transparent; height: 13px; width: 13px;}
  #toolbar-personality li:hover { background-position: bottom; }
  #toolbar-personality li#role-tab, #toolbar-personality li { left: 5px;width: 13px; }

  #toolbar-personality li#role-tab { left: 5px; width: 15px; }
  /* slide roll thing */

  #personalityFrame, #personalityFrame div.button, #personalityScroller, .section { height: 57em; }

  #personalityFrame { overflow: hidden; margin: 0px; margin-top: 5px; width: 670px; height: 467px; position: relative; }

  #personalityFrame div.button {
    position: absolute;
    top: 46%;
  }

  div.button#left { left: 15px; }
  div.button#right { right: 13px; }

  #personalityScroller {height: 467px;margin: 0px 15px 0px 15px;overflow: hidden;}

  #personalityContent {width: 5250px;}

  .personalitySection {width: 740px;margin: 15px 0px 0px 18px;float: left;}

  .personalitySection h2, .personalitySection h3 {bottom: 0px;left: 40px;}

  .personalitySection h3 {
    color: #000;
  }

  .personalitySection p, .personalitySection li { position: relative;margin: 0 0 -17px 5px;line-height: 22px;top: 412px;font-weight:400;font-size:13px;color:black; }

  .personalitySection li strong { font-size: 19px;font-weight: normal; }

  .personalitySection img { float: left;margin-right: 20px;margin-bottom: 20px; }

  #role-pane p, #role-pane li { font-weight:400; font-size:13px; color:black; }

  .flowheader{ font-size:16px; color:#305aa0; font-family:Arial; font-weight:bold; margin:10px 0px 10px 0px; }

  cards-pane
  .spiffy5{
    border-left:1px solid #DDDDDD;
    border-right:1px solid #DDDDDD;
  }
</style>

<!--personality-content-->
<div class="personality-content">
  <!--image-scorller-->
  <div class="image-scorller">
    <div class="top-curve">&nbsp;</div>
    <div class="personality-scroller-content">
      <div class="gradient-content">
        <div class="personality-scroll-content">
            <div id="personalityFrame">
                <!-- Contains the forward/back buttons, hides the scrollbar -->
                <div class="button" id="left">
                      <a href="javascript:void(0)" onClick="ScrollArrowPersonality('left','toolbar-personality','personalityScroller','role-pane');">
                          <%= image_tag("previous-blue-image.png", :alt=>"Slider Control", :onMouseDown => "this.src='/assets/previous-blue-image.png'", :onMouseUp => "this.src='/assets/previous-blue-image.png'") %>
                      </a>
                </div>

                <div id="personalityScroller">
                      <!-- Contains the horizontal scrollbar -->
                      <div id="personalityContent">
                        <% if defined?(@job_seeker_birkman_detail) and !@job_seeker_birkman_detail.nil? %>
                            <!-- Very wide to contain all the sections side-by-side -->
                            <div class="personalitySection" id="role-pane">
                                <% if seeker_role_txt_by_score(@job_seeker_birkman_detail) %>
                                  <p>
                                    <%= "#{@job_seeker.first_name.force_encoding('utf-8')}" + " " + "#{@job_seeker.last_name.force_encoding('utf-8')}" %>
                                    <%= ("(" + @job_seeker_birkman_detail.pass_birkman_code + ")" if !@job_seeker_birkman_detail.pass_birkman_code.nil?) %>
                                  </p>
                                  <% if seeker_role_txt_by_score(@job_seeker_birkman_detail) == "DOER" %>
                                    <%= image_tag("RoleInterest-Doer.jpg", :size=>"603x467", :alt=>"Doer") %>
                                  <% elsif seeker_role_txt_by_score(@job_seeker_birkman_detail) == "COMMUNICATOR" %>
                                    <%= image_tag("RoleInterest-Communicator.jpg", :size => "603x467", :alt => "Communicator") %>
                                  <% elsif seeker_role_txt_by_score(@job_seeker_birkman_detail) == "THINKER" %>
                                    <%= image_tag("RoleInterest-Thinker.jpg", :size => "603x467", :alt => "Thinker") %>
                                  <% elsif seeker_role_txt_by_score(@job_seeker_birkman_detail) == "ANALYZER" %>
                                    <%= image_tag("RoleInterest-Analyzer.jpg", :size => "603x467", :alt => "Analyzer") %>
                                  <% end %>
                                <% end %>
                            </div>
                            <div class="personalitySection" id="roleInterest-pane">
                                <% if seeker_workenv_txt_by_score(@job_seeker_birkman_detail) %>
                                  <p>
                                      <%= "#{@job_seeker.first_name.force_encoding('utf-8')}" + " " + "#{@job_seeker.last_name.force_encoding('utf-8')}" %>
                                      <%= ("(" + @job_seeker_birkman_detail.pass_birkman_code + ")" if !@job_seeker_birkman_detail.pass_birkman_code.nil?) %>
                                  </p>
                                  <% if seeker_workenv_txt_by_score(@job_seeker_birkman_detail) == "DOER" %>
                                    <%= image_tag("Doer.jpg", :size => "603x467", :alt => "Doer") %>
                                  <% elsif seeker_workenv_txt_by_score(@job_seeker_birkman_detail) == "COMMUNICATOR" %>
                                    <%= image_tag("Communicator.jpg", :size => "603x467", :alt => "Communicator") %>
                                  <% elsif seeker_workenv_txt_by_score(@job_seeker_birkman_detail) == "THINKER" %>
                                    <%= image_tag("Thinker.jpg", :size=>"622x467", :alt=>"Thinker") %>
                                  <% elsif seeker_workenv_txt_by_score(@job_seeker_birkman_detail) == "ANALYZER" %>
                                    <%= image_tag("Analyzer.jpg", :size=>"622x467", :alt=>"Analyzer") %>
                                  <% end %>
                                <% end %>
                            </div>
                        <% end %>
                      </div>
                </div>
                <div class="button" id="right">
                      <a href="javascript:void(0)" onClick="ScrollArrowPersonality('right','toolbar-personality','personalityScroller','role-pane');">
                            <%= image_tag("next-blue-image.png", :alt=>"Slider Control", :onmousedown => "this.src='/assets/next-blue-image.png'", :onmouseup => "this.src='/assets/next-blue-image.png'") %>
                      </a>
                </div>
            </div>
            <ul id="toolbar-personality">
                  <li id="role-tab" class="active">
                        <a href="javascript:void(0)" onClick="ScrollSectionPersonality('role-pane', 'personalityScroller', 'role-pane'); return false;" title="Role"></a>
                  </li>
                  <li id="roleInterest-tab" class="inactive">
                        <a href="javascript:void(0)" onClick="ScrollSectionPersonality('roleInterest-pane', 'personalityScroller', 'role-pane'); return false;" title="Role"></a>
                  </li>
            </ul>
            <div style="clear: both"></div>
        </div>
      </div>
    </div>
    <div class="bottom-curve">&nbsp;</div>
  </div>
  <!--/image-scorller-->
</div>
<!--/personality-content-->

<script type="text/javascript">
  $(function(){
    $(".profile-breadcrumb").children().each(function(){												
      if($(this).hasClass("active")){
        if($(this).children().attr("title")!="Intro"){
          $("#info-dummy").hide();
          return;
        }
        else if($(this).children().attr("title")=="Intro"){
          $("#info-dummy").show();
          return;
        }
      }
    })
  })
</script>